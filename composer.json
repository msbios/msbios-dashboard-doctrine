{
    "name": "msbios/dashboard-doctrine",
    "description": "MSBios Dashboard Doctrine",
    "license": "GPL",
    "keywords": [
        "php"
    ],
    "homepage": "https://github.com/msbios/msbios-dashboard-doctrine",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Judzhin Miles",
            "email": "info@msbios.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MSBios\\Dashboard\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MSBiosTest\\Dashboard\\Doctrine\\": "tests/"
        }
    },
    "require": {
        "zendframework/zend-component-installer": "^1.1",
        "msbios/cpanel": "^1.0",
        "msbios/cpanel-doctrine": "^1.0",
        "msbios/guard": "^1.0",
        "msbios/guard-resource": "^1.0",
        "msbios/authentication": "^1.0",
        "msbios/authentication-doctrine": "^1.0",
        "msbios/guard-resource-doctrine": "^1.0",
        "msbios/doctrine": "^1.0",
        "msbios/resource": "^1.0",
        "msbios/resource-doctrine": "^1.0",
        "msbios/theme": "^1.0",
        "msbios/navigation": "^1.0"
    },
    "require-dev": {
        "msbios/debug": "^1.0",
        "msbios/test": "^1.0"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": [
            "@cs-check",
            "phpunit --colors=always"
        ],
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v",
        "themes": "cd public && ln -s ../themes/default/public default",
        "serve": "php -S 0.0.0.0:3107 -t public public/index.php",
        "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
    },
    "extra": {
        "zf": {
            "component": "MSBios\\Application"
        }
    }
}
